     <property name="styleSheet">
      <string notr="true">QLabel {
color:black;
}

.QLineEdit {
color:black;
background-color:rgb(255, 255, 255);
}

QDateTimeEdit {
    background-color: rgb(255, 255, 255);    /* Light gray background */
    border: 2px solid rgb(86, 86, 86);   /* Gray border */
    border-radius: 0px;          /* Rounded corners */
    padding: 0px;                /* Inner padding */
    color: #333333;              /* Text color */
}

QDateTimeEdit QAbstractSpinBox::selection {
    background-color: #0066cc;     /* Blue background for highlighted text */
    color: #ffffff;                /* White color for highlighted text */
}

QDateTimeEdit:focus {
    border: 1px solid #0066cc;   /* Blue border on focus */
    background-color: #ffffff;   /* White background on focus */
}

/* Styling for the up and down arrow buttons */
QDateTimeEdit::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right; /* Position the up arrow button */
    width: 15px;
    height: 15px;
    border: none;
    background: #cccccc;         /* Gray background for button */
    border-radius: 2px;
}

QDateTimeEdit::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right; /* Position the down arrow button */
    width: 15px;
    height: 15px;
    border: none;
    background: #cccccc;         /* Gray background for button */
    border-radius: 2px;
}

/* Hover and pressed effects for the buttons */
QDateTimeEdit::up-button:hover, QDateTimeEdit::down-button:hover {
    background: #999999;         /* Darker gray on hover */
}

QDateTimeEdit::up-button:pressed, QDateTimeEdit::down-button:pressed {
    background: #666666;         /* Even darker gray on press */
}

QDateTimeEdit QAbstractItemView {
    background-color: #ffffff;   /* Background for calendar */
    border: 1px solid #cccccc;   /* Border for calendar */
    selection-background-color: #0066cc; /* Blue background for selected date */
    selection-color: #ffffff;    /* White text for selected date */
    color: #333333;              /* Default text color */
    font-size: 14px;             /* Font size */
}

/* Month and year navigation area */
QCalendarWidget QToolButton {
    background-color: #0066cc;   /* Blue background for navigation buttons */
    color: #ffffff;              /* White text color for month/year */
    border: none;
    border-radius: 3px;
    padding: 5px;
}

/* Hover effect for navigation buttons */
QCalendarWidget QToolButton:hover {
    background-color: #005bb5;   /* Darker blue on hover */
}

/* Month and year text */
QCalendarWidget QToolButton[icon-visible=&quot;false&quot;] {
    background-color: #e0e0e0;   /* Light gray background for month text */
    color: #333333;              /* Dark gray text for month name */
    font-size: 16px;             /* Increase font size for emphasis */
    font-weight: bold;           /* Bold text */
    border: none;
    border-radius: 3px;
    padding: 5px;
}

/* Navigation arrows */
QCalendarWidget QToolButton::menu-indicator {
    width: 0px;                  /* Hide dropdown arrow for month navigation */
}

QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #f5f5f5;   /* Background color for navigation bar */
}

/* Weekday header (e.g., Sun, Mon) */
QCalendarWidget QHeaderView::section {
    background-color: #cccccc;   /* Gray background for weekday header */
	border:none;
    color: #ffffff;              /* Text color for weekday header */
    padding: 5px;
    font-size: 14px;
    font-weight: bold;
}

/* Days in the calendar */
QCalendarWidget QAbstractItemView {
    background-color: #f0f0f0;   /* White background for days */
    color: #000000;              /* Black text for days */
    selection-background-color: #0066cc; /* Blue for selected day */
    selection-color: #ffffff;    /* White text for selected day */
}</string>
